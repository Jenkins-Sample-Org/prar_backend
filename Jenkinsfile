pipeline {
    agent any
    options {
        // Timeout counter starts AFTER agent is allocated
        timeout(time: 1, unit: 'SECONDS')
    }
    stages {
        stage('Roshan') {
            steps {
                echo 'Welcome Doctor'
            }
        }
        stage('Alton') {
              steps {
                  echo 'Welcome to Backend'
              }
          }
    }
}
